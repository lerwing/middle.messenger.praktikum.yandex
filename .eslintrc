{
  "extends": ["airbnb", "prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier", "import"],
  "rules": {
    "max-len": [2, 140],
    "@typescript-eslint/no-unused-vars": 2,
    "jsx-quotes": [1, "prefer-double"]
  },
  "no-underscore-dangle": false,
  "import/resolver": {
    "node": {
      "extensions": [
        ".js",
        ".ts",
        ".tsx",
        ".jsx"
      ]
    }
  }
}
